package reactorex01;

import org.reactivestreams.Subscription;

// 구독 정보
public class MySubscription implements Subscription{

	public void request(long n) {
		System.out.println("4. 구독자가 하루에 신문 " + n + "개씩 받는다네 ?? ㅋㅅㅋ");
		// n개씩 돌려주는 로직이 필요함
	}

	public void cancel() {
		// TODO Auto-generated method stub
		
	}

}
